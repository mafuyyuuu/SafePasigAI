    <font
        android:font="@font/satoshi_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/satoshi_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <font
        android:font="@font/satoshi_black"
